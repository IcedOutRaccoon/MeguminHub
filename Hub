--UI CREDITS ARE KAVO LIBRARY BY XHeptic IF INTERESTED: https://xheptcofficial.gitbook.io/kavo-library/
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
if game.PlaceId == 8365905206 then
local Window = Library.CreateLib("Tag Game", "BloodTheme")
local Tab = Window:NewTab("Farming")
Section:NewButton("Coin Farm", "Farms Coins may break if you get stuck use a noclip", function()
    while true do
	wait(2)
local modelcoin = game.Workspace.Coins:FindFirstChild("Coin")
local plr = game.Players.LocalPlayer
local char = plr.Character
local charitems = char:GetChildren("IsAPart")
charitems.CanCollide = false
char:MoveTo(modelcoin.Coin.Position)
end
end)
Section:NewButton("Safe Zone", "Keeps you in a safe zone so you can't get tagged and can farm wins, if you are a tagger it won't auto tag.", function()
    while true do
	wait(0.1)
local plr = game.Players.LocalPlayer
local char = plr.Character
char:MoveTo(game.Workspace.ShopPreview.MoveTo1.Position)
end
end)
end
